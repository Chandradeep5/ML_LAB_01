import pandas as pd
import numpy as np

data_frame=pd.read_excel(r"Lab Session1 Data.xlsx",sheet_name="Purchase data")
#data_frame.dropna(axis=1,inplace=True)
d1=data_frame.iloc[0:10,1:4]
A=d1.to_numpy()


d3=data_frame.iloc[0:10,4:5]
C=d3.to_numpy()


Ain=np.linalg.pinv(A)


X=np.matmul(Ain,C)


cost = np.linalg.pinv(A) @ C
re = np.hstack((A, C))
dimensionality=np.linalg.matrix_rank(re)


print(dimensionality)
print(cost)

